{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/mediawiki"
	],
	"globals": {
		"jQuery": false
	},
	"rules": {
		"array-bracket-spacing": "off",
		"brace-style": "off",
		"camelcase": "off",
		"computed-property-spacing": "off",
		"eqeqeq": "off",
		"no-undef": "off",
		"no-unused-vars": "off",
		"no-implied-eval": "off",
		"quote-props": "off",
		"space-before-function-paren": "off",
		"valid-jsdoc": "off",
		"block-scoped-var": "warn",
		"no-var": "warn",
		"prefer-const": "warn"
	}
}
